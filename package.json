{
	"name": "@lesjoursfr/agenda",
	"version": "1.0.3",
	"description": "Light weight job scheduler for Node.js",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lesjoursfr/agenda.git"
	},
	"homepage": "https://github.com/lesjoursfr/audio-waveform#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/audio-waveform/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": "20.x || 22.x || 24.x"
	},
	"keywords": [
		"job",
		"jobs",
		"cron",
		"delayed",
		"scheduler",
		"runner"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"check-lint": "eslint",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"build": "tsc -p tsconfig.build.json",
		"test": "mocha",
		"typedoc": "typedoc src/index.ts --out ./docs && prettier --write ./docs --ignore-path .gitignore"
	},
	"dependencies": {
		"@lesjoursfr/date": "^1.0.2",
		"@lesjoursfr/human-interval": "^1.0.2",
		"cron-parser": "^5.0.4",
		"debug": "^4.4.0",
		"luxon": "^3.5.0",
		"mongodb": "~6.14.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"@tsconfig/node20": "^20.1.4",
		"@types/debug": "^4.1.12",
		"@types/luxon": "^3.4.2",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.13.8",
		"@types/sinon": "^17.0.4",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"globals": "^16.0.0",
		"mocha": "^11.1.0",
		"mongodb": "^6.14.0",
		"mongodb-memory-server": "^10.1.4",
		"prettier": "^3.5.2",
		"sinon": "^19.0.2",
		"ts-node": "^10.9.2",
		"typedoc": "^0.27.9",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.25.0"
	},
	"packageManager": "yarn@4.6.0"
}
